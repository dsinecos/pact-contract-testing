{
  "consumer": {
    "name": "GreetingAPIConsumer"
  },
  "interactions": [
    {
      "description": "A request for Greeting",
      "providerState": "An endpoint to fetch greeting exists",
      "request": {
        "method": "GET",
        "path": "/greeting"
      },
      "response": {
        "body": {
          "language": "EN",
          "message": "Hello"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.language": {
            "match": "type"
          },
          "$.body.message": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "version": "0.9.3"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "GreetinAPI"
  }
}